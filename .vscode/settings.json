{
  "cSpell.words": [
    "adsbygoogle",
    "adsense",
    "aipictors",
    "chakra",
    "followee",
    "followees",
    "nextjs",
    "ourt",
    "prompton",
    "scta",
    "splide"
  ],
  "deno.enable": false,
  "editor.codeActionsOnSave": {
    "quickfix.biome": true,
    "source.organizeImports.biome": true
  },
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "files.associations": {
    "*.svg": "html",
    "*.xml": "html"
  },
  "files.exclude": {
    "**/node_modules": true,
    "*.lock": true,
    "*.log": true,
    "*.tsbuildinfo": true,
    ".firebase": true,
    ".idea": true,
    ".next": true,
    ".yarn": true,
    "__generated__": true,
    "dist": true,
    "next-env.d.ts": true
  },
  "search.exclude": {
    "*.lock": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "non-relative"
}
